<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationManifest>app.manifest</ApplicationManifest>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Icons\bus_ico.ico" />
    <None Remove="Icons\KTX.ico" />
    <None Remove="Icons\macro.ico" />
    <None Remove="Icons\srt_ico.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Selenium.Support" Version="4.10.0" />
    <PackageReference Include="Selenium.WebDriver" Version="4.10.0" />
    <PackageReference Include="Selenium.WebDriver.ChromeDriver" Version="114.0.5735.9000" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Icons\bus_ico.ico" />
    <Resource Include="Icons\KTX.ico" />
    <Resource Include="Icons\macro.ico" />
    <Resource Include="Icons\srt_ico.ico" />
  </ItemGroup>

</Project>
